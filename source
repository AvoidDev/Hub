CircleReach.Name = "Circle Reach"
CircleReach.Parent = Main
CircleReach.BackgroundColor3 = Color3.fromRGB(99, 99, 99)
CircleReach.Position = UDim2.new(0.10009557, 0, 0.229491383, 0)
CircleReach.Size = UDim2.new(0, 191, 0, 50)
CircleReach.Font = Enum.Font.SourceSans

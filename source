ClickToTeleport.Name = "Click To Teleport "
ClickToTeleport.Parent = Main
ClickToTeleport.BackgroundColor3 = Color3.fromRGB(99, 99, 99)
ClickToTeleport.Position = UDim2.new(0.587356687, 0, 0.229491413, 0)
ClickToTeleport.Size = UDim2.new(0, 200, 0, 50)
ClickToTeleport.Font = Enum.Font.SourceSans
ClickToTeleport.Text = "Click To Teleport Tool"
ClickToTeleport.TextColor3 = Color3.fromRGB(0, 0, 0)
ClickToTeleport.TextScaled = true
ClickToTeleport.TextSize = 1.000
ClickToTeleport.TextWrapped = true
	ClickToTeleport.MouseButton1Down:connect(function()
		mouse = game.Players.LocalPlayer:GetMouse()
		tool = Instance.new("Tool")
		tool.RequiresHandle = false
		tool.Name = "Click Teleport"
		tool.Activated:connect(function()
			local pos = mouse.Hit+Vector3.new(0,2.5,0)
			pos = CFrame.new(pos.X,pos.Y,pos.Z)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		end)
		tool.Parent = game.Players.LocalPlayer.Backpack
	end)

